{
  "parser": "@typescript-eslint/parser", 
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module", 
    "project": "./tsconfig.json"
  },
  "extends": [
    "eslint:recommended", 
    "plugin:@typescript-eslint/recommended" 
  ],
  "plugins": ["@typescript-eslint"], 
  "env": {
    "node": true,
    "browser": true
  },
  "rules": {

    "no-unused-vars": "warn",
    "@typescript-eslint/no-unused-vars": "warn"
  }
}
